{
    "name": "chrisullyott/php-ip-api",
    "type": "library",
    "description": "A PHP wrapper for ip-api.com.",
    "homepage": "https://github.com/chrisullyott/php-ip-api",
    "license": "MIT",
    "authors": [{
        "name": "Chris Ullyott",
        "email": "contact@chrisullyott.com",
        "homepage": "http://chrisullyott.com"
    }],
    "support": {
        "issues": "https://github.com/chrisullyott/php-ip-api/issues",
        "source": "https://github.com/chrisullyott/php-ip-api"
    },
    "require": {
        "php": ">=5.4",
        "rmccue/requests": "^1.7"
    },
    "require-dev": {
        "phpcompatibility/php-compatibility": "^9.3",
        "squizlabs/php_codesniffer": "^3.5",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "ChrisUllyott\\": "src/"
        }
    },
    "keywords": [
        "php",
        "ip-address",
        "ip-geolocation"
    ]
}
